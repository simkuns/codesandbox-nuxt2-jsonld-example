{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "types": [
      "@nuxt/types",
      // "nuxt-jsonld",
      "codesandbox-nuxt2-ui-example"
    ],
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    }
  },
  "vueCompilerOptions": {
    "target": 2.7
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    ".nuxt-storybook",
    "dist"
  ]
}
